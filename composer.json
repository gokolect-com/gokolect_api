{        
    "package_name": "Gokolect_api",
    "package_type": "project",
    "desc": "A social good app to help givers easily reach out to people in need",
    "keywords": [
        "give",
        "collect",
        "kolect",
        "givers",
        "giver",
        "need"
    ],
    "license_name": "MIT License",
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "phpmailer/phpmailer": "^6.0",
        "nowakowskir/php-jwt": "^2.0.1",
        "predis/predis": "^1.1",
        "symfony/flex": "^2.0.4",
        "monolog/monolog": "^2.0",
        "twig/twig": "^2.0",
        "symfony/twig-bridge": "^3",
        "wixel/gump": "v2.0.0",
        "csanquer/pdo-service-provider": "~1.1dev",
        "flutterwavedev/flutterwave-v3": "^1.0.3",        
        "mashape/unirest-php": ">=3.0"
    },
    "autoload": {
        "psr-4": {
            "Gokolect\\": "web/app/"
        }
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*",
        "phpunit/phpunit": ">=6.0",
        "mockery/mockery": ">=1.2"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
